<% if trip_article.liked_by(current_user).blank? %>
    <%= link_to "お気に入りに保存", trip_article_likes_path(trip_article), method: :post, remote: true  %>
<% else %>
    <%= link_to "保存済み", trip_article_like_path(trip_article,trip_article.liked_by(current_user)), method: :delete, remote: true %>
<% end %>